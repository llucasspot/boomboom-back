{
  "name": "boomboom-back",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --watch",
    "test": "node ace test",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,json,css,md}'",
    "format-check": "prettier --check './**/*.{js,jsx,ts,tsx,json,css,md}'",
    "typecheck": "tsc --noEmit",
    "seed:dev": "NODE_ENV=development node ace db:seed",
    "swagger:spotify:generate": "openapi-generator-cli generate -i ./app/_api/spotify/open-api-schema.yaml -g typescript-axios -o ./swagger/spotify --type-mappings=DateTime=Date --additional-properties=npmName=swagger-spotify-backend,supportsES6=true,withInterfaces=true",
    "swagger:spotify:link": "(cd ./swagger/spotify && yarn build); yarn link && cd ../../ && yarn link swagger-spotify-backend",
    "swagger:spotify:update": "yarn swagger:spotify:generate && yarn swagger:spotify:link"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#start/*": "./start/*.js",
    "#tests/*": "./tests/*.js",
    "#config/*": "./config/*.js",
    "#utils/*": "./app/utils/*.js",
    "#api/*": "./app/_api/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.1.0",
    "@adonisjs/eslint-config": "^1.2.1",
    "@adonisjs/prettier-config": "^1.2.1",
    "@adonisjs/tsconfig": "^1.2.1",
    "@faker-js/faker": "^8.4.0",
    "@japa/api-client": "^2.0.2",
    "@japa/assert": "^2.1.0",
    "@japa/plugin-adonisjs": "^3.0.0",
    "@japa/runner": "^3.1.1",
    "@openapitools/openapi-generator-cli": "^2.9.0",
    "@swc/core": "^1.3.105",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.6",
    "@types/uuid": "^9.0.7",
    "eslint": "^8.56.0",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@adonisjs/ally": "^5.0.2",
    "@adonisjs/auth": "^9.0.2",
    "@adonisjs/core": "^6.2.1",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/lucid": "^19.0.0",
    "@adonisjs/session": "^7.1.1",
    "@vinejs/vine": "^1.7.0",
    "adonisjs-6-swagger": "^1.0.1",
    "axios": "^1.6.5",
    "better-sqlite3": "^9.3.0",
    "luxon": "^3.4.4",
    "mysql2": "^3.8.0",
    "reflect-metadata": "^0.2.1",
    "sharp": "^0.33.2",
    "uuid": "^9.0.1"
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/app"
  },
  "prettier": "@adonisjs/prettier-config"
}
